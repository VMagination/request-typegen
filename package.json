      "types": "./lib/vite.d.ts"
    },
    "./react-native": {
      "import": "./lib/react-native.js",
      "types": "./lib/react-native.d.ts"
    }
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vite-plugin-dts": "^2.2.0",
    "vite-plugin-fs": "^1.0.0-beta.6"
  },
  "peerDependencies": {
    "vite-plugin-fs": "^1.0.0-beta.6"
  },
  "peerDependenciesMeta": {
    "vite-plugin-fs": {
      "optional": true
