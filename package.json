{
  "name": "@vmagination/request-typegen",
  "version": "0.0.1",
  "type": "module",
  "main": "lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib",
    "typeGen-placeholder.d.ts"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
